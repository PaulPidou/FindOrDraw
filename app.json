{
  "expo": {
    "name": "FindOrDraw",
    "slug": "findordraw",
    "platforms": ["android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_square.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD139"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.cognitivecompany.findordraw",
      "permissions": ["CAMERA"]
    }
  }
}
